@model Project_MVC.ModelView.CustomerViewModel
@using Project_MVC.Models;

@{
    ViewBag.Title = "LoginHome";
}


<h2>Login</h2>

@using (Html.BeginForm("Login", "Login", FormMethod.Post))
{
    <table class="form-group">
        <tr>
            <td><lable>Customer Name</lable></td>
            <td>@Html.TextBoxFor(m=>m.Customer.UserName) @Html.ValidationMessageFor(x => x.Customer.UserName)</td>
        </tr>
        <tr>
            <td><lable>Password</lable></td>
            <td>@Html.PasswordFor(m=>m.Customer.Password)</td>
        </tr>
        <tr>
           
            <td>
                <input type="submit" class="btn btn-info" value="Login" />
            </td>
        </tr>
    </table>

}

<p>
  
    @foreach(User obj in Model.Customers)
    {
        @obj.UserName;
    }
    <div style="color:red">@ViewBag.message</div>
</p>